    "import:shapefiles:dry": "node scripts/import-shapefiles.js --dry-run --limit=100"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^5.0.2",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.90.20",
    "@turf/turf": "^6.5.0",
    "@types/leaflet": "^1.9.21",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.21.0",
    "recharts": "^2.10.0",
    "tailwind-merge": "^2.2.0",
    "zustand": "^4.4.7"
